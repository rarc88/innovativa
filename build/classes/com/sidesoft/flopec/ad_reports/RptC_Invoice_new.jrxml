	<queryString>
		<![CDATA[select
invoice.c_invoice_id,
partner.taxid as "partner_ruc",
coalesce(to_char(user_.name),'') || ' / ' || coalesce(to_char(sba.name),'') as "writting_by",
coalesce(to_char(invoice.documentno),'') as "documentoinvoice" ,
coalesce(to_char(invoice.poreference),'') as "documentoinvoiceref" ,
extract(year from invoice.dateacct) as anio,
coalesce(to_char(invoice.description),'') as invoicedescription,
coalesce(to_char(partnerlocation.name),'')  as localizacion_user,
coalesce(to_char(partner.name),'') as name1,
coalesce(to_char(partner.name2),'') as NameFiscal,
coalesce(to_char(partner.name),'') as NNF,
coalesce(to_char(user_.title || ' - ' || invoice.documentno || ' - ' ||  extract(year from invoice.dateacct)),'')  AS TitleDocumYear,
coalesce(to_char(user_.title),'') as title,
coalesce(to_char(user_.name),'') as "user",
currency.iso_code as valuecurrency,
coalesce((invoice.grandtotal-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingincome),0))-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingvat),0))),0) as priceactual ,
'USD' as currencyconvert,
coalesce(to_char((ssfl_convert_numbertoletters((invoice.grandtotal-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingincome),0))-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingvat),0)))) || '          ' || currency.description)),'') as priceinletters,
coalesce(to_char(bank.bank_name),'') || coalesce(' - ' || to_char( case when bank.bankaccounttype='S' then 'Cuenta de Ahorros' when bank.bankaccounttype='C' then 'Cuenta Corriente' end ),'') || coalesce(to_char(''),'')  as bank_name,
(bank.accountno ||  coalesce(' IBAN: ' ||to_char(bank.em_ssfl_iban),' IBAN: ') ||  coalesce(' SWIFTCODE: ' || to_char(bank.swiftcode),' SWIFTCODE:')) as accountno,
TO_CHAR(invoice.dateinvoiced,'dd-MM-yyyy') AS dateinvoiced2,
TO_CHAR(invoice.created,'dd-MM-yyyy') AS dateinvoiced,
TO_CHAR(invoice.dateacct,'dd-MM-yyyy') AS dateacct,
USER_.name as createdby_name,
invoice.em_ssfl_observation as observation,
sba.name as "area",
co.documentno as "orderdocument",
coalesce(to_number((case when invoice.c_currency_id <> '100'  then cnvr.multiplyrate end)),0) as factorconversion,
case when invoice.c_currency_id ='100'
then coalesce((invoice.grandtotal-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingincome),0))-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingvat),0))),0)
else coalesce((invoice.grandtotal-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingincome),0))-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingvat),0))),0)
 * (coalesce(to_number((case when invoice.c_currency_id <> '100'  then cnvr.multiplyrate end)),0))
end as GrandTotalConvert,
case when invoice.c_currency_id ='100'
then coalesce((invoice.grandtotal-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingincome),0))-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingvat),0))),0)
else coalesce((invoice.grandtotal-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingincome),0))-to_number(coalesce(to_number(EM_Sswh_Totalwithholdingvat),0))),0)
 * (coalesce(to_number((case when invoice.c_currency_id <> '100'  then cnvr.multiplyrate end)),0))
end as valor
from c_invoice invoice
inner join c_invoiceline invoiceline on invoiceline.c_invoice_id = invoice.c_invoice_id
inner join c_bpartner_location partnerlocation on partnerlocation.c_bpartner_location_id = invoice.c_bpartner_location_id
inner join c_bpartner partner on partner.c_bpartner_id =  invoice.c_bpartner_id
inner join ad_user user_ on user_.ad_user_id = invoice.createdby
inner join c_currency currency on currency.c_currency_id = invoice.c_currency_id
left join C_BP_BankAccount bank on  invoice.EM_Sswh_Bankaccount_ID = bank.C_BP_BankAccount_ID
left join c_bpartner bpc on bpc.c_bpartner_id = invoice.createdby
left join sfb_budget_area sba on sba.sfb_budget_area_id = invoice.em_sfb_budget_area_id
left join c_order co on co.c_order_id = invoice.c_order_id
left join c_conversion_rate cnvr on cnvr.c_currency_id = invoice.c_currency_id and (invoice.dateacct between cnvr.validfrom and cnvr.validto )
WHERE invoice.c_invoice_id = $P{DOCUMENT_ID}
and invoice.docstatus = 'CO']]>
	</queryString>
