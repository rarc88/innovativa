	<queryString>
		<![CDATA[SELECT    org.name as organizacion,
	fr.fin_reconciliation_id,
	fp.fin_financial_account_id,
	fp.paymentdate as fechapagocobro,
	fr.statementdate as fechareconciliacion,
	fpm.name as tipopago,
	fp.documentno as numerodocumento,
	cbp.name as beneficiario,
	fp.description as detalle,
	CASE WHEN fp.isReceipt='Y' THEN amount ELSE 0 END as deposito,
	CASE WHEN fp.isReceipt='N' THEN amount ELSE 0 END as reintegro,
(	select  ABS( (A.INITIALAMT+A.AMTACCTDR) - A.AMTACCTCR ) AS SALDO_FINAL
	from(
		  SELECT sum(case when (facct.DATEACCT < '2000-01-01' or (facct.DATEACCT = '2000-01-01' and facct.FACTACCTTYPE = 'N')) then facct.AMTACCTDR - facct.AMTACCTCR else 0 end) as initialamt,
		  sum(case when (facct.DATEACCT >= '2000-01-01' AND facct.FACTACCTTYPE not in('O', 'R', 'C')) or (facct.DATEACCT = '2000-01-01' and facct.FACTACCTTYPE = 'N') then facct.AMTACCTDR else 0 end) as AMTACCTDR,
		  sum(case when (facct.DATEACCT >= '2000-01-01' AND facct.FACTACCTTYPE not in('O', 'R', 'C')) or (facct.DATEACCT = '2000-01-01' and facct.FACTACCTTYPE = 'N') then facct.AMTACCTCR else 0 end) as AMTACCTCR,
		  facct.ACCOUNT_ID AS ID
		FROM fin_financial_account_acct ffaa
		LEFT JOIN  C_ValidCombination cvc ON cvc.C_ValidCombination_id = ffaa.fin_withdrawal_acct
		LEFT JOIN FACT_ACCT facct ON facct.account_id = cvc.account_id
		WHERE ffaa.fin_financial_account_id = fr.fin_financial_account_id
		AND ffaa.isactive = 'Y'
		AND facct.DATEACCT >= '2000-01-01'
		AND facct.DATEACCT <= fr.statementdate
		GROUP BY facct.ACCOUNT_ID
	)A
) as saldobancos
FROM fin_reconciliation fr
LEFT JOIN FIN_Payment fp ON fp.fin_financial_account_id = fr.fin_financial_account_id
LEFT JOIN c_bpartner cbp ON cbp.c_bpartner_id = fp.c_bpartner_id
LEFT JOIN fin_paymentmethod fpm ON fpm.fin_paymentmethod_id = fp.fin_paymentmethod_id
LEFT JOIN ad_org org ON org.ad_org_id = fr.ad_org_id
WHERE fr.fin_reconciliation_id = $P{RECONCILIATION_ID}
AND (fp.status = 'PPM' OR fp.status = 'RPR')
AND fp.paymentdate <= fr.statementdate]]>
	</queryString>
