<!--0341D0506A9749518B07E91079949D3D-->  <NAME><![CDATA[opcrmLeadAccessValidation]]></NAME>
<!--0341D0506A9749518B07E91079949D3D-->  <TYPE><![CDATA[S]]></TYPE>
<!--0341D0506A9749518B07E91079949D3D-->  <CODE><![CDATA[ad_user.ad_user_id IN (
SELECT acc.ad_user_id 
FROM opcrm_lead_access acc
INNER JOIN ad_user u ON acc.ad_user_id = u.ad_user_id
WHERE acc.crm_user_id = @ad_user_id@
) OR ad_user.em_opcrm_assigned_to = @ad_user_id@ 
OR @ad_user_id@='100' 
OR @ad_user_id@ IN (SELECT u.ad_user_id FROM opcrm_superusers u WHERE u.ad_client_id = @ad_client_id@)]]></CODE>
