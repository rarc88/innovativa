	<queryString>
		<![CDATA[select
ordercab.c_order_id,
partner.taxid as "partner_ruc",
coalesce(to_char(user_.name),'') || ' / ' || coalesce(to_char(sba.name),'') as "writting_by",
coalesce(to_char(ordercab.documentno),'') as "documentoorder" ,
coalesce(to_char(ordercab.poreference),'') as "documentoorderref" ,
extract(year from ordercab.dateacct) as anio,
coalesce(to_char(ordercab.description),'') as orderdescription,
coalesce(to_char(partnerlocation.name),'')  as localizacion_user,
coalesce(to_char(partner.name),'') as name1,
coalesce(to_char(partner.name2),'') as NameFiscal,
coalesce(to_char(partner.name),'') as NNF,
coalesce(to_char(user_.title || ' - ' || ordercab.documentno || ' - ' ||  extract(year from ordercab.dateacct)),'')  AS TitleDocumYear,
coalesce(to_char(user_.title),'') as title,
coalesce(to_char(user_.name),'') as "user",
currency.iso_code as valuecurrency,
coalesce((ordercab.grandtotal),0) as priceactual ,
'USD' as currencyconvert,
coalesce(to_char((ssfl_convert_numbertoletters(ordercab.grandtotal) || '          ' || currency.description)),'') as priceinletters,
coalesce(to_char(bank.bank_name),'') || coalesce(' - ' || to_char( case when bank.bankaccounttype='S' then 'Cuenta de Ahorros' when bank.bankaccounttype='C' then 'Cuenta Corriente' end ),'') || coalesce(to_char(''),'')  as bank_name,
(bank.accountno ||  coalesce(' IBAN: ' ||to_char(bank.em_ssfl_iban),' IBAN: ') ||  coalesce(' SWIFTCODE: ' || to_char(bank.swiftcode),' SWIFTCODE:')) as accountno,
TO_CHAR(ordercab.dateordered,'dd-MM-yyyy') AS dateorder2,
TO_CHAR(ordercab.created,'dd-MM-yyyy') AS dateorder,
TO_CHAR(ordercab.dateacct,'dd-MM-yyyy') AS dateacct,
USER_.name as createdby_name,
ordercab.description as observation,
sba.name as "area",
co.documentno as "orderdocument",
coalesce(to_number((case when ordercab.c_currency_id <> '100'  then cnvr.multiplyrate end)),0) as factorconversion,
case when ordercab.c_currency_id ='100'
then coalesce((ordercab.grandtotal),0)
else coalesce((ordercab.grandtotal),0)
* (coalesce(to_number((case when ordercab.c_currency_id <> '100'  then cnvr.multiplyrate end)),0))
end as GrandTotalConvert,
case when ordercab.c_currency_id ='100'
then coalesce((ordercab.grandtotal),0)
else coalesce((ordercab.grandtotal),0)
 * (coalesce(to_number((case when ordercab.c_currency_id <> '100'  then cnvr.multiplyrate end)),0))
end as valor
from c_order ordercab
inner join c_orderline ordercabline on ordercabline.c_order_id = ordercab.c_order_id
inner join c_bpartner_location partnerlocation on partnerlocation.c_bpartner_location_id = ordercab.c_bpartner_location_id
inner join c_bpartner partner on partner.c_bpartner_id =  ordercab.c_bpartner_id
inner join ad_user user_ on user_.ad_user_id = ordercab.createdby
inner join c_currency currency on currency.c_currency_id = ordercab.c_currency_id
left join C_BP_BankAccount bank on  partner.c_bpartner_id = bank.c_bpartner_id
left join c_bpartner bpc on bpc.c_bpartner_id = ordercab.createdby
left join sfb_budget_area sba on sba.sfb_budget_area_id = ordercab.em_sfb_budget_area_id
left join c_order co on co.c_order_id = ordercab.c_order_id
left join c_conversion_rate cnvr on cnvr.c_currency_id = ordercab.c_currency_id and (ordercab.dateacct between cnvr.validfrom and cnvr.validto )
WHERE ordercab.c_order_id = $P{DOCUMENT_ID}
and ordercab.docstatus = 'CO']]>
	</queryString>
