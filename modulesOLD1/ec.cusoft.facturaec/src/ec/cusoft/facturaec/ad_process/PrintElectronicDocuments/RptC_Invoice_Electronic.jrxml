<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="RptC_Invoice_ElectronicAg" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" isIgnorePagination="true">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="568"/>
	<parameter name="DOCUMENT_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT C_INVOICE.C_INVOICE_ID as "c_invoice_id",
C_BPARTNER.name  as "bp_name",
C_BPARTNER.TAXID AS "cif",
AD_USER.NAME AS "contact_name",
CLI.ADDRESS1  as "dir_matriz",
CLI.ADDRESS1  as "dir_sucursal",
coalesce(to_char(c_bpartner_location.name),to_char(c_location.address1))  as "bp_direccion",
COALESCE(to_char(C_BPARTNER_LOCATION.PHONE),'-') AS "phone",
C_INVOICE.DATEINVOICED as "dateinvoiced",
C_INVOICE.DOCUMENTNO as "documentno",
COALESCE(to_char(C_INVOICE.DESCRIPTION), '-') AS "description_cab",
C_BPARTNER.VALUE AS "code_bpartner",
AD_CLIENT.DESCRIPTION AS "entity",
C_Location_Description(AD_ORGINFO.C_LOCATION_ID) AS "org_location",
C_CURRENCY.ISO_CODE AS "currency_code",
C_CURRENCY.DESCRIPTION AS "symbol_largo",
C_CURRENCY.CURSYMBOL AS "symbol",
C_PAYMENTTERM.NAME AS "payterm",
C_PAYMENTTERM.NETDAYS AS "numero_dias",
AD_ORGINFO.ad_org_id AS "organizationid",
C_INVOICE.docstatus AS "status",
AD_ORG.NAME AS "org_name",
AD_ORGINFO.TAXID AS "org_taxid",
C_BPARTNER.TAXID AS "bp_taxid",
SHOWLOGO as "showlogo",
SHOWCOMPANYDATA as "showcompanydata",
HEADERMARGIN as "headermargin",
C_INVOICE.ISSOTRX as "issotrx",
C_INVOICELINE.QTYINVOICED as "qtyinvoiced",
C_INVOICELINE.PRICEACTUAL as "priceactual",
C_INVOICELINE.LINENETAMT as "linenetamt",
coalesce(to_char(C_INVOICELINE.DESCRIPTION), '-') as "description_line",
M_PRODUCT.VALUE as "value" , fep.value_ie as "producto",
C_INVOICE.TOTALLINES as "totallines",
C_INVOICE.GRANDTOTAL as "grandtotal",
'' AS "letras",
COALESCE(to_char(M_INOUT.DOCUMENTNO),' - ') AS "referencia",
 COALESCE(to_char(C_ORDER.poreference),'-') AS "referencia_pedido",
 to_char(C_INVOICE.DATEINVOICED,'dd-MM-yyyy')  as "fecha_Actual"
 , EEI_RETURNDOCUMENTNO(C_INVOICE.documentno) AS "ref_invoice",
c_invoice.EM_Eei_Fechaautotext as "fecha_autorizacion",
coalesce(to_char(c_invoice.EM_Eei_Rsi_Auth_No),to_char(c_invoice.EM_Eei_Numauto)) as "num_autorizacion",
 coalesce(to_char(c_invoice.EM_Eei_Codigo),'') as "cod_acceso",
 C_BPARTNER.ad_language as "ad_language",
ad_user.email as "bp_mail",
coalesce(to_number(IMP.BASECERO),0) as "basecero",
coalesce(to_number(imp.iva12),0) as "iva12"
,coalesce(to_number(imp.base12),0) as "base12",
case
( EEI_Param_Facturae.environment  )
when '1' then 'Pruebas'
when '2' then 'ProducciÃ³n'
end as "environment",
case
( EEI_Param_Facturae.Sending_Type )
when '1' then 'Normal'
when '2' then 'Indisponibilidad de Sistema/Contingencia'
end as "emision",
fep.value_ie as "codigo_prin",
fep.value_ie as "codigo_aux",
CASE WHEN DV.EM_SSFL_PRINT = 'N' THEN VOYAGE.EM_SSFL_NAME ELSE IV.voyageno END AS "identifier",

CASE WHEN DV.EM_SSFL_PRINT = 'N' THEN CCT.NAME || ' ' ||
to_char(CASE
('(' || (CASE 	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' THEN RECAP1.NAME
				WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' THEN RECAP2.NAME
				WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' THEN RECAP3.NAME
		END) || ')')
		WHEN '()' THEN ''
		ELSE ('(' || (CASE 	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' THEN RECAP1.NAME
						WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' THEN RECAP2.NAME
						WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' THEN RECAP3.NAME
					END) || ')')
END)
ELSE
CV.NAME|| ' ' ||RV.VALUE
END AS "head_costcenter",
to_char(CASE 	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' THEN RECAP1.NAME
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' THEN RECAP2.NAME
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' THEN RECAP3.NAME
END) AS "name_recap",
to_char(CASE 	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' THEN RECAP1.VALUE
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' THEN RECAP2.VALUE
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' THEN RECAP3.VALUE
END) AS "value_recap",
to_char(CASE WHEN DV.EM_SSFL_PRINT = 'N' THEN
		CASE 	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' and c_invoice.em_ssre_isrefund = 'N' THEN RECAP1.DESCRIPTION
		WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' and c_invoice.em_ssre_isrefund = 'Y' THEN RECAP1.DESCRIPTION_REM
		WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' and c_invoice.em_ssre_isrefund = 'N' THEN RECAP2.DESCRIPTION
		WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' and c_invoice.em_ssre_isrefund = 'Y' THEN RECAP2.DESCRIPTION_REM
		WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' and c_invoice.em_ssre_isrefund = 'N' THEN RECAP3.DESCRIPTION
		WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' and c_invoice.em_ssre_isrefund = 'Y' THEN RECAP3.DESCRIPTION_REM
		END
	ELSE
		CASE WHEN c_invoice.em_ssre_isrefund = 'N' THEN RV.DESCRIPTION
		WHEN c_invoice.em_ssre_isrefund = 'Y' THEN RV.DESCRIPTION_REM END
	END ) AS "description_recap",
  CASE WHEN DV.EM_SSFL_PRINT = 'N' THEN
ssfl_return_name_month((CASE 	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' THEN RECAP1.CONTRACTDATE
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' THEN RECAP2.CONTRACTDATE
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' THEN RECAP3.CONTRACTDATE
END),C_BPARTNER.ad_language)||
to_char((CASE 	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '1' THEN RECAP1.CONTRACTDATE
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '2' THEN RECAP2.CONTRACTDATE
	WHEN c_invoice.EM_SSFL_TYPE_RECAP = '3' THEN RECAP3.CONTRACTDATE
END),'dd, yyyy')
ELSE
CASE WHEN RV.CONTRACTDATE IS NOT NULL THEN to_char(RV.CONTRACTDATE,'dd')||', '||ssfl_return_name_month(RV.CONTRACTDATE,C_BPARTNER.ad_language)||to_char(RV.CONTRACTDATE,'yyyy') ELSE NULL END
END AS "contractdate_recap",
C_BPARTNER.em_eei_detail_ei as "em_eei_detail_ei",
C_INVOICE.EM_SSFL_DESCRIPTIONVOYAGE as "em_ssfl_descriptionvoyage",
C_INVOICE.DESCRIPTION AS "invoice_description",
CASE WHEN DV.EM_SSFL_PRINT = 'N' THEN VOYAGE.EM_SSFL_CUSTOMER_VOYAGE_NO
ELSE IV.customer_voyageno END as "customer_voyage_no",
TO_CHAR(SSFL_RETURN_NAME_MONTH(C_INVOICE.EM_SSFL_BILLING_PERIOD_FROM,C_BPARTNER.ad_language)) || TO_CHAR(C_INVOICE.EM_SSFL_BILLING_PERIOD_FROM,'dd, yyyy "' || Case when C_BPARTNER.ad_language= 'es_EC' then 'a las' when C_BPARTNER.ad_language= 'en_US' then 'to' end || '" HH24:MI:SS') AS "billing_period_from_invoice",
TO_CHAR(SSFL_RETURN_NAME_MONTH(C_INVOICE.EM_SSFL_BILLING_PERIOD_TO,C_BPARTNER.ad_language)) || TO_CHAR(C_INVOICE.EM_SSFL_BILLING_PERIOD_TO,'dd, yyyy "' || Case when C_BPARTNER.ad_language= 'es_EC' then 'a las' when C_BPARTNER.ad_language= 'en_US' then 'to' end || '" HH24:MI:SS') AS "billing_period_to_invoice",C_INVOICELINE.line as "line",CASE WHEN DV.EM_SSFL_PRINT = 'N' THEN  C_INVOICELINE.description ELSE PV.name END as "descriptionline" ,ir.poreference as "nofact_compra",pr.taxid as "tercero_compra"  FROM C_INVOICE LEFT JOIN C_BPARTNER ON C_INVOICE.C_BPARTNER_ID = C_BPARTNER.C_BPARTNER_ID
	 LEFT JOIN AD_USER ON C_INVOICE.AD_USER_ID = AD_USER.AD_USER_ID
	 LEFT JOIN C_BPARTNER_LOCATION ON C_INVOICE.C_BPARTNER_LOCATION_ID = C_BPARTNER_LOCATION.C_BPARTNER_LOCATION_ID AND C_BPARTNER_LOCATION.isbillto='Y'
	 LEFT JOIN C_LOCATION ON C_BPARTNER_LOCATION.C_LOCATION_ID = C_LOCATION.C_LOCATION_ID
	 LEFT JOIN C_REGION ON C_LOCATION.C_REGION_ID = C_REGION.C_REGION_ID
	 LEFT JOIN C_COUNTRY ON C_LOCATION.C_COUNTRY_ID = C_COUNTRY.C_COUNTRY_ID
	 LEFT JOIN C_PAYMENTTERM ON C_INVOICE.C_PAYMENTTERM_ID = C_PAYMENTTERM.C_PAYMENTTERM_ID
	 LEFT JOIN C_CURRENCY ON C_INVOICE.C_CURRENCY_ID = C_CURRENCY.C_CURRENCY_ID
	 LEFT JOIN C_INVOICELINE ON C_INVOICE.C_INVOICE_ID = C_INVOICELINE.C_INVOICE_ID
	 LEFT JOIN M_PRODUCT ON C_INVOICELINE.M_PRODUCT_ID = M_PRODUCT.M_PRODUCT_ID
	 LEFT JOIN M_INOUTLINE ON C_INVOICELINE.M_INOUTLINE_ID = M_INOUTLINE.M_INOUTLINE_ID
	 LEFT JOIN M_INOUT ON M_INOUTLINE.M_INOUT_ID= M_INOUT.M_INOUT_ID
	 LEFT JOIN C_POC_DOCTYPE_TEMPLATE ON C_POC_DOCTYPE_TEMPLATE.C_DOCTYPE_ID = C_INVOICE.C_DOCTYPE_ID
	 LEFT JOIN C_ORDERLINE ON C_INVOICELINE.C_ORDERLINE_ID=C_ORDERLINE.C_ORDERLINE_ID
	 LEFT JOIN C_ORDER ON C_ORDERLINE.C_ORDER_ID=C_ORDER.C_ORDER_ID
	 LEFT JOIN AD_USER USU ON C_INVOICE.salesrep_id=USU.AD_USER_ID
   left join c_bpartner bp2 on bp2.c_bpartner_id = c_invoice.c_invoice_id
   left join c_bpartner_location bpl on bpl.c_bpartner_id = bp2.c_bpartner_id and bpl.isbillto = 'Y'
   left join c_location cl2 on cl2.c_location_id = bpl.c_location_id
   left join sswh_taxpayer on sswh_taxpayer.sswh_taxpayer_id = C_BPARTNER.em_sswh_taxpayer_id
   left join ad_org on ad_org.ad_org_id = c_invoice.ad_org_id
   left join ad_orginfo  on ad_orginfo.ad_org_id = ad_org.ad_org_id
   left join ad_client on ad_client.ad_client_id = c_invoice.ad_client_id
   LEFT JOIN C_LOCATION CLI ON CLI.C_LOCATION_ID = AD_ORGINFO.C_LOCATION_ID
   LEFT JOIN C_INVOICELINE ILR ON C_INVOICELINE.EM_SSRE_REFUNDEDINVLINE_REF_ID = ILR.C_INVOICELINE_ID
   LEFT JOIN C_INVOICE IR ON ILR.C_INVOICE_ID = IR.C_INVOICE_ID
   LEFT JOIN C_BPARTNER PR ON IR.C_BPARTNER_ID = PR.C_BPARTNER_ID
   LEFT JOIN ssfl_invoice_voyage IV on C_INVOICE.C_INVOICE_ID = IV.C_INVOICE_ID
   LEFT JOIN SSFL_RECAP RV on RV.SSFL_RECAP_ID = IV.recap_id
   LEFT JOIN ssfl_port PV on IV.ssfl_port_id = PV.ssfl_port_id
   LEFT JOIN c_doctype DV on C_INVOICE.c_doctype_id = DV.c_doctype_id
   LEFT JOIN C_COSTCENTER CV ON CV.C_COSTCENTER_ID = IV.C_COSTCENTER_ID
   LEFT JOIN (select c_invoice_id, sum(basecero) as basecero,sum(base12) as base12,sum(iva) as iva12 from (select a.c_invoice_id,c.name,c.taxid,a.documentno,d.taxbaseamt,d.taxamt, e.rate,
case e.rate when 0 then d.taxbaseamt else 0 end basecero,
case e.rate when 12 then d.taxbaseamt else 0 end base12,
case e.rate when 12 then d.taxamt else 0 end iva,
((case e.rate when 0 then d.taxbaseamt else 0 end) + (case e.rate when 12 then d.taxbaseamt else 0 end) + case e.rate when 12 then d.taxamt else 0 end) grandtotal
from c_invoice a left join c_invoiceline b on a.c_invoice_id = b.c_invoice_id
left join c_bpartner c on a.c_bpartner_id = c.c_bpartner_id
left join c_invoicelinetax d on b.c_invoiceline_id = d.c_invoiceline_id
left join c_tax e on d.c_tax_id = e.c_tax_id) impuestos
group by c_invoice_id) IMP ON IMP.C_INVOICE_ID = C_INVOICE.C_INVOICE_ID
left join eei_product fep on fep.m_product_id = M_PRODUCT.m_product_id
left join ad_orginfo orgf on orgf.ad_org_id = c_invoice.ad_org_id
left join USER2 VOYAGE on c_invoice.user2_id = voyage.user2_id
LEFT JOIN C_COSTCENTER CCT ON CCT.C_COSTCENTER_ID = VOYAGE.EM_SSFL_C_COSTCENTER_ID--****
LEFT JOIN SSFL_RECAP RECAP1 ON VOYAGE.EM_SSFL_VALUERECAP1 = RECAP1.SSFL_RECAP_ID
LEFT JOIN SSFL_RECAP RECAP2 ON VOYAGE.EM_SSFL_VALUERECAP2 = RECAP2.SSFL_RECAP_ID
LEFT JOIN SSFL_RECAP RECAP3 ON VOYAGE.EM_SSFL_VALUERECAP3 = RECAP3.SSFL_RECAP_ID,EEI_Param_Facturae
 	WHERE C_INVOICE.ad_client_id = AD_CLIENT.ad_client_id
AND C_INVOICE.C_INVOICE_ID = $P{DOCUMENT_ID}
AND C_INVOICE.DOCSTATUS = 'CO'
AND AD_ORG.ad_org_id = AD_ORGINFO.ad_org_id
AND AD_ORG.ad_org_id = (SELECT o.AD_ORG_ID FROM AD_ORG o JOIN AD_OrgType t USING (AD_ORGTYPE_ID)
                        WHERE AD_ISORGINCLUDED(C_INVOICE.AD_ORG_ID, o.ad_org_id, C_INVOICE.ad_client_id)<>-1
                              AND (t.IsLegalEntity='Y' OR t.IsAcctLegalEntity='Y'))
ORDER BY C_BPARTNER.TAXID]]>
	</queryString>
