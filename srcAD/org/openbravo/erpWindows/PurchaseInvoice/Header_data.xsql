    <Sql><![CDATA[
        SELECT DocumentNo FROM C_Invoice WHERE isActive='Y' AND C_Invoice_ID = ?  
    ]]></Sql>
    <Parameter name="C_Invoice_ID"/>
  </SqlMethod>

   <SqlMethod name="selectActDefM_Locator_ID" type="preparedStatement" return="String" default="">
